<div class="container">
<div class="row">
<div class="col-md-2 col-md-offset-4">
<h1>Admin Sign In</h1>
<p><%= form_tag '/login', method: :post do %></p>
	<%= label_tag :email %>
	<%= email_field_tag :email %>

	<%= label_tag :password %>
	<%= password_field_tag :password %>
	<p></p>

	<%= submit_tag "Login" %>

	<% end %>

	<br>
	<br>
</div>
</div>
</div>	